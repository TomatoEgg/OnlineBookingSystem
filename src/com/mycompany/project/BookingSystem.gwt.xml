<module>
	<inherits name="com.google.gwt.user.User"/>
	<inherits name="com.google.gwt.user.theme.standard.Standard"/>
	<inherits name="com.google.gwt.place.Place"/>
	<inherits name="com.google.gwt.activity.Activity"/>
	<inherits name="com.smartgwt.SmartGwt"/>
	<entry-point class="com.mycompany.project.client.BookingSystem"/>
	
    <!-- Use ClientFactoryImpl by default -->
    <replace-with class="com.mycompany.project.client.ClientFactoryImpl">
    <when-type-is class="com.mycompany.project.client.ClientFactory"/>
    </replace-with>
    
    <!-- Specify the paths for translatable code 
         Note: If you add any packages in which some java classes should be translated
         to JS, add a <source path='package name'> below.
    -->
    <source path='client'/>
    <source path='shared'/>
    <source path='resources'/>
</module>